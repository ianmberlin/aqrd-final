clear all
set more off
cap log close
version 14
pause on

cd "$root/data"

*Some name cleaning.
use ./stata_data/FORBES_1982_2017.dta, clear 

local d = c(current_date)
log using "../log/name_cleaning_new`d'.log", replace 


gen family = 0
replace Name = strtrim(strproper(Name))
replace Name = subinstr(Name, "  ", " ", .)
replace family = 1 if regexm(Name, "Family")==1
replace Name = subinstr(Name, "& Family", "", .) if family==1
foreach y in "Fam Ily" "Fami Ly" "& Family" "And Family" "Family" "family"{
replace family = 1 if regexm(Name, "`y'")==1
replace Name = subinstr(Name, "`y'", "", .)
}

drop if mi(Name)

replace Name = strtrim(Name)
replace Name = stritrim(Name)

gen NameReverse = strreverse(Name)

replace lastname = strreverse(substr(NameReverse, 1, strpos(NameReverse, " ")))
replace lastname = strtrim(lastname)
replace firstname = substr(Name, 1, strpos(Name, " "))
replace firstname = strtrim(firstname)

drop if strlen(Name) == 1

gen midname = substr(Name, strpos(Name, " "), strlen(Name) - strpos(NameReverse, " ") - strpos(Name, " ") + 1)
order lastname firstname midname
gen firstlast = firstname + " " + lastname

